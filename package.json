{
  "name": "feng3d",
  "version": "0.8.0",
  "description": "Web3D Engine",
  "standalone": true,
  "main": "src/index.ts",
  "types": "src/index.ts",
  "module": "src/index.ts",
  "author": "feng",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf \"{lib,dist,public}\"",
    "build": "vite build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "types": "tsc",
    "watch": "tsc -w",
    "lint": "eslint --ext .js --ext .ts src test --ignore-path .gitignore --max-warnings 0",
    "lintfix": "npm run lint -- --fix",
    "docs": "typedoc",
    "release": "npm run clean && npm run lint && npm run docs && npm run build && npm run types && npm publish",
    "prepublishOnly": "node scripts/prepublish.js",
    "postpublish": "node scripts/postpublish.js"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/feng3d/feng3d.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "lib",
    "src"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "@vitest/ui": "^0.26.2",
    "cross-env": "7.0.3",
    "eslint": "8.12.0",
    "rimraf": "3.0.2",
    "tslib": "^2.4.0",
    "typedoc": "0.23.23",
    "typescript": "5.1.3",
    "vite": "^3.0.7",
    "vitest": "^0.26.2"
  },
  "dependencies": {
    "@feng3d/earcut": "^0.0.1",
    "@feng3d/event": "^0.8.2",
    "@feng3d/path": "^0.0.3",
    "@feng3d/polyfill": "^0.8.1",
    "stats.js": "^0.17.0"
  }
}