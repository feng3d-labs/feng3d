{
  "name": "@feng3d/event",
  "version": "0.3.4",
  "description": "",
  "standalone": true,
  "types": "dist/index.d.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "bundle": "dist/browser/index.js",
  "bundleModule": "dist/browser/index.mjs",
  "bundleOutput": {
    "name": "feng3d"
  },
  "scripts": {
    "start": "npm run watch",
    "clean:build": "rimraf dist out",
    "prebuild": "npm run clean:build",
    "build": "npm run build:dev",
    "build:prod": "cross-env NODE_ENV=production rollup -c",
    "build:dev": "rollup -c",
    "build:types": "rimraf out && tsc && node ./scripts/apiExtractorLernaMmonorepo.js",
    "watch": "rollup -cw",
    "lint": "eslint --ext .js --ext .ts src scripts --ignore-path .gitignore --max-warnings 0",
    "lintfix": "npm run lint -- --fix",
    "docs": "typedoc src/index.ts",
    "predist": "rimraf dist/*",
    "dist": "npm run build:types && npm run docs",
    "postdist": "copyfiles -f bundles/*/dist/browser/* dist && copyfiles -f \"packages/**/dist/browser/*\" dist/packages",
    "prerelease": "npm run clean:build",
    "postversion": "npm run build:prod && npm run build:types",
    "release": "lerna publish --exact --force-publish"
  },
  "author": "feng",
  "files": [
    "dist/",
    "*.d.ts"
  ],
  "pre-commit": [
    "lintfix"
  ],
  "devDependencies": {
    "@lerna/batch-packages": "^3.13.0",
    "@lerna/filter-packages": "^3.13.0",
    "@lerna/project": "^3.13.1",
    "@microsoft/api-extractor": "^7.9.11",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^7.2.0",
    "lerna": "^3.13.3",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.32.1",
    "rollup-plugin-jscc": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-node": "^9.0.0",
    "typedoc": "^0.20.10",
    "typescript": "^4.1.3"
  },
  "gitHead": "7a968379054159e52658180408513eeb95004348"
}