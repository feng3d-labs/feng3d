{
    "source": {
        "include": [
            "dist/compiled"
        ],
        "excludePattern": "(node_modules|lib|test|useDeprecated|canvas)"
    },
    "plugins": [
        "@pixi/jsdoc-template/plugins/es6-fix"
    ],
    "opts": {
        "recurse": true,
        "destination": "./dist/types",
        "outFile": "feng3d.d.ts",
        "template": "./node_modules/tsd-jsdoc/dist"
    }
}